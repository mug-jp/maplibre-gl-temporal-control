{
	"name": "maplibre-gl-temporal-control",
	"type": "module",
	"version": "1.1.1",
	"description": "Temporal Control plugin for Maplibre GL JS",
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1",
		"build": "vite build -c vite.config.example.ts",
		"build:pkg": "vite build && tsc",
